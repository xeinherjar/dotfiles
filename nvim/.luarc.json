{
  "runtime.version": "LuaJIT",
  "runtime.path": [
    "lua/?.lua",
    "lua/?/init.lua"
  ],
  "diagnostics.globals": ["vim", "Snacks"],
  "workspace.checkThirdParty": false,
  "workspace.library": [
    "$VIMRUNTIME",
    "${workspaceFolder}",
    "${workspaceFolder}/lua",
    "${env:VIMRUNTIME}/lua",
    "${env:XDG_DATA_HOME}/nvim/lazy",
    "${env:HOME}/.local/share/nvim/lazy"
  ]
}
