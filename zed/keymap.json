[
  {
    "context": "Editor && vim_mode == normal",
    "bindings": {
      "space f": "editor::Format",
      "space f f": "file_finder::Toggle",
      "space space": "command_palette::Toggle",
      "[ b": "pane::ActivatePreviousItem",
      "] b": "pane::ActivateNextItem",
      "[ j": "pane::GoBack",
      "] j": "pane::GoForward",
      "g i": "editor::GoToImplementation",
      "g d": "editor::GoToDefinition",
      "g r": "editor::FindAllReferences",
      "space f b": "tab_switcher::Toggle",
      "space c a": "editor::ToggleCodeActions",
      "space b d": "pane::CloseActiveItem",
      "cmd-w h": "workspace::ActivatePaneLeft",
      "cmd-w j": "workspace::ActivatePaneDown",
      "cmd-w k": "workspace::ActivatePaneUp",
      "cmd-w l": "workspace::ActivatePaneRight",
      "cmd-w H": "workspace::SwapPaneLeft",
      "cmd-w J": "workspace::SwapPaneDown",
      "cmd-w K": "workspace::SwapPaneUp",
      "cmd-w L": "workspace::SwapPaneRight",
      "cmd-w |": "workspace::NewFileSplitVertical",
      "cmd-w -": "workspace::NewFileSplitHorizontal",
      "space n t": "workspace::ToggleLeftDock",
      "space f r g": "workspace::NewSearch"
    }
  },
  {
    "context": "Editor && vim_mode == insert",
    "bindings": {
      "j j": ["workspace::SendKeystrokes", "escape"]
    }
  }
]
